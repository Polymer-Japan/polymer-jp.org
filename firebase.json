{
  "hosting": {
    "public": "build/default",
    "rewrites": [
      {
        "source": "!/__/**",
        "function": "app"
      },{
        "source": "**/!(*.js|*.html|*.css|*.json|*.ico|*.svg|*.png|*.jpg|*.jpeg|*.txt|apple-app-site-association)",
        "function": "app"
      }
    ]
  }
}
