{
  "hosting": {
    "public": "build/default",
    "rewrites": [
      {
        "source": "/sitemap.xml",
        "function": "sitemap"
      }, {
        "source": "/!(*.*|apple-app-site-association)",
        "function": "app"
      },{
        "source": "/**/!(*.*|apple-app-site-association)",
        "function": "app"
      }
    ]
  }
}
