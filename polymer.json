{
  "entrypoint": "index.html",
  "shell": "src/polymer-jp.html",
  "fragments": [
    "src/polymer-jp-marked.html",
    "src/polymer-jp-marked-edit.html",
    "src/polymer-jp-md-components.html",
    "src/polymer-jp-donate.html",
    "src/polymer-jp-theme.html",
    "src/polymer-jp-inquery.html"
  ],
  "sources": [
    "assets/**/*",
    "404.html",
    "robots.txt",
